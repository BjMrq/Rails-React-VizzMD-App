  <h2 class="patient-ref" >New Snapshot</h2>
  <h2 class="top-no-react-element-right"><%= link_to "See Evolution >", user_patient_bodies_path(current_user, @patient), class: "evolution-link", id:"not-to-be-printed" %></h2>
