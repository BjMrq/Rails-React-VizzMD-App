<div class="modal fade" id=<%= modal_data_target %> tabindex="-1" role="dialog" aria-labelledby="editUserModallabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="gradient-square">
        <h5 id="editUserModallabel" class="gradient-square-text">Edit Patient Infos</h5>
      </div>
      <div class="modal-body scrollbar-custom">
        <div class="form-group">
          <div class="col-md">
            <div class="input-group">
              <div class="input-group-addon"></div>
            </div>
          </div>
        </div>
        <%= render "patients/new_patient_form", patient: patient, url: user_patient_path(current_user, patient)%>
      </div>
    </div>
  </div>
</div>
