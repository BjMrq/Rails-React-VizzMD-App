<div class="patient-n-evolution">
  <h2 class="patient-ref" id="rotate">Patient: <%= @body.patient.hospital_patient_id %></h2>
  <h2 class="patient-ref" id="not-to-be-printed">Update Snapshot</h2>
  <h2 class="top-no-react-element-right"><%= link_to "See Evolution >", user_patient_bodies_path(current_user, @body.patient), class: "evolution-link", id:"not-to-be-printed" %></h2>
</div>
<div id=dashboard data-user='[{"id":<%=current_user.id%>,"token":"<%=current_user.authentication_token %>","email":"<%=current_user.email%>"}]' data-save="<%= @save %>" data-patient="<%= @body.patient.id %>" data-muscles="<%= @body.muscles.to_json %>" data-hospital-id="<%= @body.patient.hospital_patient_id %>" data-traitment='{"traitment":"<%=@body.traitment.gsub("\n",'')%>","note":"<%=@body.note.gsub("\n",'') %>","date":"<%=@body.date_data_capture%>"}'></div>
