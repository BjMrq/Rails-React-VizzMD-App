<div id=dashboard
data-user='[{"id":<%=current_user.id%>,"token":"<%=current_user.authentication_token %>","email":"<%=current_user.email%>"}]'
data-save="<%= data_save %>"
data-patient="<%= @patient.id %>"
data-hospital-id="<%= @patient.hospital_patient_id %>"
  <% if data_save == "update"%>
    data-muscles="<%= @body.muscles.to_json %>"
    data-reflexes="<%= @body.reflexes.to_json %>"
    data-traitment='{"traitment":"<%=@body.traitment.gsub("\n",'')%>","note":"<%=@body.note.gsub("\n",'') %>","date":"<%=@body.date_data_capture%>"}'
  <% end %>
></div>
