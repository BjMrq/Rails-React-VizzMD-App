<div class="patient-n-evolution">
  <h2 className="patient-ref" id="rotate">Patient: <%= @patient.hospital_patient_id %></h2>
  <h2 class="patient-ref" >Evolution</h2>
  <h2 class="top-no-react-element-right"><%= link_to "New Snapshot +", new_user_patient_body_path(current_user, @patient), class: "evolution-link" %></h2>
</div>

<div id=evolution data-user='[{"id":<%=current_user.id%>,"token":"<%=current_user.authentication_token %>","email":"<%=current_user.email%>"}]' data-bodies="<%= @bodies.to_json %>"></div>

<h5 class="edit-on-evolution">Click on a snapchot to edit it</h5>
